{
  "version": 2,
  "name": "report-mark-ii-s2",
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/auth/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/templates/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/reports/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/users/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/logs/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/files/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/dist/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "regions": ["iad1"]
}
